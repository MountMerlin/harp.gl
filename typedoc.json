{
    "name": "harp.gl",
    "entryPoints": ["."],
    "entryPointStrategy": "expand",
    "out": "dist/doc",
    "exclude": [
        "**/node_modules/**",
        "**/@xyzmaps/harp.gl/lib/*.ts",
        "**/test/**/*.ts",
        "**/www/**/*.ts",
        "**/dist/**/*.ts",
        "**/scripts/*.ts",
        "**/@xyzmaps/generator-harp.gl/**/*.ts",
        "**/+(*Test.ts|index_node.ts|index-worker.ts|*_generated.ts|publish-npm.ts|changelog.ts|doc-snippets.ts)"
    ],
    "includes": "dist/doc-snippets",
    "media": "dist/media",
    "readme": "README.md",
    "excludePrivate": true,
    "excludeExternals": true,
}
